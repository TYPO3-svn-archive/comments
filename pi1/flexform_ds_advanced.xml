<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>0</langChildren>
	</meta>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:comments/pi1/locallang.xml:sheet.advanced</sheetTitle>
			<cshFile>LLL:EXT:comments/pi1/locallang_csh.xml</cshFile>
		</TCEforms>
		<type>array</type>
		<el>
			<commentsPerPage>
				<TCEforms>
				<exclude>1</exclude>
				<label>LLL:EXT:comments/pi1/locallang.xml:tt_content.tx_comments_pi1.commentsPerPage</label>
				<config>
					<type>input</type>
					<default>10</default>
					<eval>trim,lower,num</eval>
					<size>7</size>
					<max>5</max>
					<range>
						<lower>1</lower>
					</range>
				</config>
				</TCEforms>
			</commentsPerPage>
			<closeCommentsAfter>
				<TCEforms>
				<exclude>1</exclude>
				<label>LLL:EXT:comments/pi1/locallang.xml:tt_content.tx_comments_pi1.closeCommentsAfter</label>
				<config>
					<type>input</type>
					<default></default>
					<eval>trim,lower,alphanum</eval>
					<size>15</size>
				</config>
				</TCEforms>
			</closeCommentsAfter>
			<enableRatings>
				<TCEforms>
				<exclude>1</exclude>
				<label>LLL:EXT:comments/pi1/locallang.xml:tt_content.tx_comments_pi1.enableRatings</label>
				<displayCond>EXT:ratings:LOADED:true</displayCond>
				<config>
					<type>check</type>
					<default></default>
				</config>
				</TCEforms>
			</enableRatings>
			<reverseSorting>
				<TCEforms>
				<exclude>1</exclude>
				<label>LLL:EXT:comments/pi1/locallang.xml:tt_content.tx_comments_pi1.reverseSorting</label>
				<config>
					<type>check</type>
					<default></default>
				</config>
				</TCEforms>
			</reverseSorting>
		</el>
	</ROOT>
</T3DataStructure>
